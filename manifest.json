{
  "manifest_version": 2,
  "name": "Brug Net",
  "version": "1.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "48": "bug.png"
    }
  },
  "icons": {
    "48": "bug.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
